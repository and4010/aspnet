@model CHPOUTSRCMES.Web.ViewModels.YszmpckqViewModel
@{
    ViewBag.Title = "板令對照";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="@Url.Content("~/Content/Yszmpckq.css")" rel="stylesheet" type="text/css" />

@section scripts {
    <script src="~/Scripts/Yszmpckq.js"></script>
    <script src="~/Scripts/moment.js"></script>
    
}
<div class="box box-primary" style="padding:20px;">
    <div class="box-body">
        <div class="row row-std">
            <div class="form-inline">
                <div class="form-group col col-md-3 ui-widget">
                    @Html.LabelFor(model => model.SelectedOrganization, new { @class = "input-lg" })
                    @Html.DropDownListFor(model => model.SelectedOrganization, Model.OrganizationNameItems, new { @class = "form-control", id = "ddlOrganization", autocomplete = "off" })
                </div>

                <div class="form-group col col-md-3 ui-widget">
                    @Html.LabelFor(model => model.SelectedOspSubinventory, new { @class = "input-lg" })
                    @Html.DropDownListFor(model => model.SelectedOspSubinventory, Model.OspSubinventoryNameItems, new { @class = "form-control", id = "ddlOspSubinventory", autocomplete = "off" })
                </div>

                <div class="form-group col col-md-3 ui-widget">
                    @Html.LabelFor(model => model.SelectedPstyp, new { @class = "input-lg" })
                    @Html.DropDownListFor(model => model.SelectedPstyp, Model.PstypNameItems, new { @class = "form-control", id = "ddlPstyp", autocomplete = "off" })
                </div>

            </div>
        </div>
    </div>
    <div class="box-footer">
        <div class="row row-std">
            <div class="btn-group col-md-1">
                <button id="btnSearch" class="btn btn-primary externalBtn">查詢</button>
            </div>
        </div>
    </div>
</div>
<div class="box box-primary" style="padding:20px;">
    <div class="box-body">
        <div class="row row-std">

            <div class="col-sm-12 col-md-12">
                <table class="table table-striped table-bordered table-hover dataTable" id="YszmpckqDataTablesBody">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayName("庫存組織")
                            </th>
                            <th>
                                @Html.DisplayName("庫存組織名稱")
                            </th>
                            <th>
                                @Html.DisplayName("加工廠")
                            </th>
                            <th>
                                @Html.DisplayName("加工廠名稱")
                            </th>
                            <th>
                                @Html.DisplayName("紙別")
                            </th>
                            <th>
                                @Html.DisplayName("紙別中文名稱")
                            </th>
                            <th>
                                @Html.DisplayName("紙別英文名稱")
                            </th>
                            <th>
                                @Html.DisplayName("基重上限")
                            </th>
                            <th>
                                @Html.DisplayName("基重下限")
                            </th>
                            <th>
                                @Html.DisplayName("令重上限")
                            </th>
                            <th>
                                @Html.DisplayName("令重下限")
                            </th>
                            <th>
                                @Html.DisplayName("包數")
                            </th>
                            <th>
                                @Html.DisplayName("每包張數")
                            </th>
                            <th>
                                @Html.DisplayName("每件令數")
                            </th>
                            <th>
                                @Html.DisplayName("建立人員")
                            </th>
                            <th>
                                @Html.DisplayName("建立日期")
                            </th>
                            <th>
                                @Html.DisplayName("更新人員")
                            </th>
                            <th>
                                @Html.DisplayName("更新日期")
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
